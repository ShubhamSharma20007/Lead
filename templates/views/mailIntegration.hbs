                                    </div>

                                    <div class="section social">
                                        <span class="material-icons"> people </span>
                                        <h4>Social</h4>
                                    </div>

                                    <div class="section promotion">
                                        <span class="material-icons"> local_offer </span>
                                        <h4>Promotions</h4>
                                    </div>

                                    <div class="section_1">
                                        <i class="fa-brands fa-google"></i>
                                    </div>
                                </div>
                                <!-- Section Ends -->

                                <!-- Email List rows starts -->
                                <div class="emailList__list Primary">

                                    <!-- Email Row Starts -->
                                    <div class="emailRow">
                                        <div class="emailRow__options">
                                            <input type="checkbox" name="" id="" />
                                            <span class="material-icons"> star_border </span>
                                            <span class="material-icons"> label_important </span>
                                        </div>

                                        <h3 class="emailRow__title">Google1</h3>

                                        <div class="emailRow__message">
                                            <h4>
                                                Login on New Device<span class="emailRow__description">
                                                    - is this your Device ?
                                                </span>
                                            </h4>
                                        </div>

                                        <p class="emailRow__time">2am</p>
                                    </div>
                                    <!-- Email Row Ends -->
                                </div>
                                <!-- Email List rows Ends -->


                                <!-- Email List rows starts -->
                                <div class="emailList__list socials ">

                                    <!-- Email Row Starts -->
                                    <div class="emailRow">
                                        <div class="emailRow__options">
                                            <input type="checkbox" name="" id="" />
                                            <span class="material-icons"> star_border </span>
                                            <span class="material-icons"> label_important </span>
                                        </div>

                                        <h3 class="emailRow__title">Google2</h3>

                                        <div class="emailRow__message">
                                            <h4>
                                                Login on New Device<span class="emailRow__description">
                                                    - is this your Device ?
                                                </span>
                                            </h4>
                                        </div>

                                        <p class="emailRow__time">2am</p>
                                    </div>
                                    <!-- Email Row Ends -->
                                </div>
                                <!-- Email List rows Ends -->

                                <!-- Email List rows starts -->
                                <div class="emailList__list promotions ">

                                    <!-- Email Row Starts -->
                                    <div class="emailRow">
                                        <div class="emailRow__options">
                                            <input type="checkbox" name="" id="" />
                                            <span class="material-icons"> star_border </span>
                                            <span class="material-icons"> label_important </span>
                                        </div>

                                        <h3 class="emailRow__title">Google3</h3>

                                        <div class="emailRow__message">
                                            <h4>
                                                Login on New Device<span class="emailRow__description">
                                                    - is this your Device ?
                                                </span>
                                            </h4>
                                        </div>

                                        <p class="emailRow__time">2am</p>
                                    </div>
                                    <!-- Email Row Ends -->
                                </div>
                                <!-- Email List rows Ends -->
                            </div>
                        </div>
                    </div>

                    <!-- Footer -->
                    {{>footer}}
                    <!-- / Footer -->

                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
    </div>


</body>


<script>
    document.addEventListener('DOMContentLoaded', function () {
        let primary = document.querySelector('.primary');
        let social = document.querySelector('.social');
        let promotion = document.querySelector('.promotion');
        let primaryList = document.querySelector('.emailList__list.Primary');
        let socialList = document.querySelector('.emailList__list.socials');
        let promotionList = document.querySelector('.emailList__list.promotions');
        socialList.style.display = 'none';
        promotionList.style.display = 'none';
        primary.addEventListener('click', function () {

            primaryList.style.display = "block";
            socialList.style.display = 'none';
            promotionList.style.display = 'none';

            document.querySelectorAll('.section').forEach(function (el) {
                el.classList.remove('section__selected');
            });

            primary.classList.add('section__selected');
        });

        social.addEventListener('click', function () {

            primaryList.style.display = "none";
            socialList.style.display = 'block';
            promotionList.style.display = 'none';

            document.querySelectorAll('.section').forEach(function (el) {
                el.classList.remove('section__selected');
            });

            social.classList.add('section__selected');
        });

        promotion.addEventListener('click', function () {

            primaryList.style.display = "none";
            socialList.style.display = 'none';
            promotionList.style.display = 'block';

            document.querySelectorAll('.section').forEach(function (el) {
                el.classList.remove('section__selected');
            });

            promotion.classList.add('section__selected');
        });
    });


</script>


<script src="/scripts/customizeleadform.js"></script>
